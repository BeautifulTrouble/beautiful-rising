{
  "name": "beautiful-rising",
  "version": "0.0.1",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" \"npm run sass\" ",    
    "server": "concurrently \"npm run tsc:w\" \"serve\" \"npm run sass\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "sass": "node-sass -w styles.scss -o ."
  },
